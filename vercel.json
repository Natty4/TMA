{
    "builds": [
      {
        "src": "Abooking/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
      },
      {
        "src": "manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      }
    
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Abooking/wsgi.py"
      },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        { "src": "/(.*)", "dest": "manage.py" }
    ]

}
